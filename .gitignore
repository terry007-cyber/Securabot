# Ignore the Python Virtual Environment
# This is machine-specific and can be huge. Your friends will create their own.
venv/

# Ignore the secret API key file
# CRITICAL: NEVER, EVER UPLOAD THIS FILE.
.env

# Ignore Python cache files
# These are temporary files generated by Python.
__pycache__/
*.pyc

# Ignore PyInstaller build artifacts
# These are generated when you build the .exe and are not source code.
dist/
build/
*.spec

# Ignore VS Code editor settings (optional but good practice)
.vscode/